package RMIStudent;
import java.rmi.*;
import java.rmi.server.*;
import java.io.*;
import java.util.ArrayList;
import java.util.Scanner;
public class StudentRMI_IPML extends UnicastRemoteObject implements RmiInterface {
	protected StudentRMI_IPML() throws RemoteException {
		super();
	}

	@Override
	public void SendStudent(ArrayList<Student> student) throws RemoteException {
		ByteArrayOutputStream baos = new ByteArrayOutputStream();
        ObjectOutputStream oos;
        try {
            oos = new ObjectOutputStream(baos);
             oos.writeObject(student);
        } catch (IOException ex) {
            System.out.print("Error: "+ex);
        }
        byte[] data = baos.toByteArray();
        return data;
	}

	@Override
	public byte[] ReceiveStuddent(ArrayList<Student> student) throws RemoteException {
		// TODO Auto-generated method stub
		
	}
	
	
}
